    public function doSearch( value : int , _begin : int , _end : int ) returns int:
        var middle = ( _begin + _end ) / 2;
        var foundVal = doSearch( value , _begin , middle );
        if (foundVal == 0)
            foundVal = doSearch( value , middle + 1 , _end );
        return foundVal + self.data.length;
    end

